exports[`Bash switches to system node: Bash 1`] = `
"set -e
eval "$(fnm env)"
fnm install v10.10.0
fnm use v10
if [ "$(node --version)" != "v10.10.0" ]; then
  echo "Expected node version to be v10.10.0. Got $(node --version)"
  exit 1
fi
fnm use system
if [ "$(node --version)" != "custom" ]; then
  echo "Expected node version to be custom. Got $(node --version)"
  exit 1
fi"
`;

exports[`Fish switches to system node: Fish 1`] = `
"fnm env | source
fnm install v10.10.0
fnm use v10
set ____test____ (node --version)
if test "$____test____" != "v10.10.0"
  echo "Expected node version to be v10.10.0. Got $____test____"
  exit 1
end
fnm use system
set ____test____ (node --version)
if test "$____test____" != "custom"
  echo "Expected node version to be custom. Got $____test____"
  exit 1
end"
`;

exports[`PowerShell switches to system node: PowerShell 1`] = `
"$ErrorActionPreference = "Stop"
fnm env | Out-String | Invoke-Expression
fnm install v10.10.0
fnm use v10
if ( "$(node --version)" -ne "v10.10.0" ) { echo "Expected node version to be v10.10.0. Got $(node --version)"; exit 1 }
fnm use system
if ( "$(node --version)" -ne "custom" ) { echo "Expected node version to be custom. Got $(node --version)"; exit 1 }"
`;

exports[`Windows Command Prompt switches to system node: Windows Command Prompt 1`] = `
"FOR /f "tokens=*" %i IN ('fnm env') DO CALL %i
fnm install v10.10.0
fnm use v10
node --version | findstr v10.10.0
if %errorlevel% neq 0 (
  echo Expected node version to be v10.10.0
  exit 1
)
fnm use system
node --version | findstr custom
if %errorlevel% neq 0 (
  echo Expected node version to be custom
  exit 1
)"
`;

