{
  "source": "./package.json",
  "override": {
    "build": "refmterr dune build --profile release",
    "resolutions": {
      "@opam/conf-openssl": {
        "source": "no-source:",
        "override": {
          "dependencies": {
            "@opam/conf-pkg-config": "1.1",
            "esy-openssl": "esy-packages/esy-openssl#f731e9c"
          },
          "buildEnv": {
            "CFLAGS": "-I$OPENSSL_INCLUDE_PATH $CFLAGS",
            "LDFLAGS": "-L$OPENSSL_LIB_PATH $LDFLAGS"
          }
        }
      },
      "@opam/ssl": {
        "source": "ulrikstrid/ocaml-ssl:ssl.opam#bff2ae0",
        "override": {
          "dependencies": {
            "@opam/conf-openssl": "*",
            "esy-openssl": "esy-packages/esy-openssl#f731e9c"
          },
          "exportedEnv": {
            "CAML_LD_LIBRARY_PATH": {
              "val": "#{self.lib / 'ssl' : $CAML_LD_LIBRARY_PATH}",
              "scope": "global"
            }
          }
        }
      }
    }
  }
}
